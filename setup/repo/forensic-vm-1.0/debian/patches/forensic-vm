+    #context = {
+    #    "token": "",
+    #    "host": "localhost",
+    #    "port": 5901,        
+    #    "password": ''
+    #}
+
+
+    iso_dir = '/forensicVM/mnt/iso'
+
+    iso_files = []
+    for file in os.listdir(iso_dir):
+        if file.endswith('.iso'):
+            iso_files.append(file)
+
+    iso_files.sort()
+    return render(request, 'novnc/vnc.html', {'iso_files': iso_files})
