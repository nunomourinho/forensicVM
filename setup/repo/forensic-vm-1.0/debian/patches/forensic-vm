+
+    function lookup(k) { return k ? {keysym: k, keyname: keynames ? keynames[k] : k} : undefined; }
+    return {
+        fromUnicode : function(u) { return lookup(codepoints[u]); },
+        lookup : lookup
+    };
+})();
