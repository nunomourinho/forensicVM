#!/bin/bash
echo "interceptado" >> /tmp/qemu-forensicVM.txt
echo $@ >> /tmp/qemu-forensicVM.txt
if [[ $1 == "convert" ]] && [[ ${10} == *"tmp"* ]] && [[ ${11} == *"forensicVM"* ]] && [[ -f "/tmp/qemu-img-cp-now" ]]; then

    mv ${10} ${11}
    touch ${10}
    rm /tmp/qemu-img-cp-now
    exit 0
else
    /usr/bin/qemu-img.forensicVM $@
    erro=$?
fi
exit $erro
